                             Margin="10,10,0,10"
                             VirtualizingStackPanel.IsVirtualizing="True"
                             VirtualizingStackPanel.VirtualizationMode="Recycling"
                             Width="400"
                             SelectionChanged="SelectSpell_SelectionChanged"
                             Focusable="False"
                             Grid.Row="0"
                             Grid.RowSpan="6"
                             Grid.Column="0"
                             Grid.ColumnSpan="2"/>
